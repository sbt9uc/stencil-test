{
  "name": "stencil-two",
  "version": "1.0.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-two/stencil-two.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "private": true,
  "homepage": "https://github.com/sbt9uc/stencil-test",
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook-a": "start-storybook -s dist",
    "storybook-b": "start-storybook -p 6006 -s dist",
    "storybook-c": "start-storybook -p 6006 -c .storybook -s www"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@stencil/core": "^2.7.0",
    "@storybook/addon-knobs": "^6.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/html": "^6.3.12",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
